#include "main.h"


beginseg
	name "code"
	flags BOOT OBJECT
	entry boot
	stack g_boot_stack + STACKSIZE
	include "codesegment.o"
	include "$(ROOT)/usr/lib/PR/rspboot.o"
	include "$(ROOT)/usr/lib/PR/gspF3DEX2.fifo.o"
	include "$(ROOT)/usr/lib/PR/gspF3DEX2.xbus.o"
endseg

beginseg
	name "static"
	flags OBJECT
	number STATIC_SEGMENT
	include "static.o"
endseg

beginseg
	name "rsp_framebuffer"
	flags OBJECT
	number RSP_FB_SEGMENT
	include "rsp_framebuffer.o"
endseg

beginwave
	name "main"
	include "code"
	include "static"
	include "rsp_framebuffer"
endwave
